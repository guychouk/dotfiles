[alias]
  a        = add
  d        = diff
  m        = merge
  f        = fetch
  b        = branch
  s        = status
  r        = reset
  bd       = branch -D
  rh       = reset --hard
  pl       = pull
  ps       = push
  cl       = clone
  rb       = rebase
  rbi      = rebase -i
  c        = commit
  ca       = commit -a
  cm       = commit -m
  cam      = commit -a -m
  sp       = stash pop
  sl       = stash list
  sa       = stash apply
  pr       = !gh pr
  co       = checkout
  new      = checkout -b
  rao      = remote add origin
  rso      = remote show origin
  rsuo     = remote set-url origin
  pso      = push origin
  psd      = push --delete origin
  psu      = push --set-upstream origin
  psom     = push origin master
  unstage  = restore --staged
  assume   = update-index --assume-unchanged
  unassume = update-index --no-assume-unchanged
  lp       = log --pretty=format:\"%h - %cn, %cd : %s\"
  ignored  = !git ls-files -v | grep "^[[:lower:]]"
