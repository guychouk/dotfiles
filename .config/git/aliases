[alias]
  a        = add
  ap       = add -p
  b        = branch
  bd       = branch -D
  c        = commit
  ca       = commit -a
  cm       = commit -m
  cam      = commit -a -m
  can      = commit --amend --no-edit
  co       = checkout
  cl       = clone
  d        = diff
  f        = fetch
  m        = merge
  new      = checkout -b
  pl       = pull
  pr       = !gh pr
  ps       = push
  psf      = push -f
  pso      = push origin
  psd      = push --delete origin
  psu      = push --set-upstream origin
  rs       = restore --staged
  rb       = rebase
  rba      = rebase --abort
  rbc      = rebase --continue
  rbi      = rebase -i
  rao      = remote add origin
  rso      = remote show origin
  rset     = remote set-url origin
  s        = status
  sp       = stash pop
  sl       = stash list
  sa       = stash apply
  # scripts
  rbm      = !git checkout master && git pull && git checkout - && git rebase master
  cub      = !git remote prune origin && git branch --merged master | grep -v '^[ *]*master$' | xargs git branch -d
